<ul class="category-list">
    {{#each categorys}}
    <li class="category">
        <button class="previous-card"></button>
        <p class="category-discription">{{category}}</p>
        <button class="look-all">Дивитися всі</button>
        <button class="next-card"></button>
        <ul class="card-list">
            {{#each cards}}
            <li class="card">
                {{> ./CARD}}
            </li>
            {{/each}}
        </ul>
    </li>
    {{/each}}
</ul>